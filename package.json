{
  "name": "arc",
  "version": "0.1.0",
  "private": true,
  "main": "src",
  "scripts": {
    "start": "node .",
    "test": "jest",
    "coverage": "npm test -- --coverage && opn coverage/lcov-report/index.html",
    "dev": "concurrently --kill-others 'nodemon .' 'node webpack/webpack-dev-server'",
    "lint": "eslint 'src/**/*.js' 'src-clean/**/*.js' 'webpack/**/*.js'",
    "clean": "del 'dist/!(.git*|Procfile)**'",
    "copy": "copyfiles -u 1 public/* public/**/* dist",
    "build": "npm run clean && npm run copy && cross-env NODE_ENV=production webpack --config webpack/webpack.config"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "modulePathIgnorePatterns": [
      "<rootDir>/src-clean"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js"
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "concurrently": "^3.1.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.1.2",
    "del-cli": "^0.2.0",
    "dotenv-safe": "^2.3.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "jest-cli": "^16.0.1",
    "mockgoose": "^6.0.8",
    "nock": "^9.0.0",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "^1.15.2",
    "bodymen": "^1.0.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "csurf": "^1.9.0",
    "express": "^4.14.0",
    "express-force-ssl": "^0.3.2",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "morgan": "^1.7.0",
    "mongoose": "^4.6.5",
    "mongoose-keywords": "^0.3.1",
    "querymen": "^2.1.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.6.0",
    "redux-form": "^6.1.1",
    "redux-form-submit": "^0.1.2",
    "redux-saga": "^0.12.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "styled-components": "^1.0.7",
    "url-loader": "^0.5.7",
    "validator": "^6.1.0",
    "webpack": "^1.13.2",
    "webpack-isomorphic-tools": "^2.6.3"
  }
}
